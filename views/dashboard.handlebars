<h1>Your Dashboard</h1>
<a href="/post/new">New Post</a>
{{#each posts}}
<div>
  <h2>{{this.title}}</h2>
  <p>{{this.content}}</p>
  <a href="/post/{{this.id}}/edit">Edit</a>
  <button data-id="{{this.id}}" class="delete-post">Delete</button>
</div>
{{/each}}
<script src="/js/delete-post.js"></script>
